{
  "include": [
    "src",
    "tests",
    "infra",
    "adapters"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.venv",
    "**/node_modules",
    "**/.pytest_cache",
    "**/.mypy_cache",
    "**/.ruff_cache",
    "build",
    "dist"
  ],
  "venvPath": ".",
  "venv": ".venv",
  "extraPaths": [
    "src",
    "infra",
    "adapters"
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.13",
  "typeCheckingMode": "basic"
}
